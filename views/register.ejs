    <script>
      document.addEventListener('DOMContentLoaded', function () {
        const form = document.getElementById('delg_from');

        form.addEventListener('submit', async function (event) {
          event.preventDefault(); // Prevent default form submission

          const formData = new FormData(form);
          const jsonData = {};

          formData.forEach((value, key) => {
            // Handle array fields (checkboxes)
            if (key.endsWith('[]')) {
              key = key.slice(0, -2); // Remove '[]' from the key
              if (!jsonData[key]) {
                jsonData[key] = [];
              }
              jsonData[key].push(value);
            } else {
              jsonData[key] = value;
            }
          });

          // Extract priceId from the URL path
          const urlPath = window.location.pathname;
          const priceId = urlPath.split('/').pop(); // Get the last part of the URL

          // Prepare the data to be sent to the API
          const apiData = {
            priceId: priceId,
            data: JSON.stringify(jsonData),
          };

          try {
            // Send the data to the API using async/await
            const response = await fetch(
              'https://backend.gaganahuja.com/api/v1/register',
              {
                method: 'POST',
                headers: {
                  'Content-Type': 'application/json',
                },
                body: JSON.stringify(apiData),
              }
            );

            const data = await response.json();
            console.log('Success:', data);

            // Clear the form
            form.reset();

            // Show success popup
            showSuccessPopup();
          } catch (error) {
            console.error('Error:', error);
            // Handle error (e.g., show an error message to the user)
            showErrorPopup();
          }
        });
      });

      function showSuccessPopup() {
        const popup = document.createElement('div');
        popup.className = 'popup success-popup';
        popup.innerHTML = `
    <div class="popup-content">
      <h2>Success!</h2>
      <p>Your registration has been submitted successfully.</p>
      <button onclick="closePopup(this.parentElement.parentElement)">Close</button>
    </div>
  `;
        document.body.appendChild(popup);
      }

      function showErrorPopup() {
        const popup = document.createElement('div');
        popup.className = 'popup error-popup';
        popup.innerHTML = `
    <div class="popup-content">
      <h2>Error</h2>
      <p>There was an error submitting your registration. Please try again.</p>
      <button onclick="closePopup(this.parentElement.parentElement)">Close</button>
    </div>
  `;
        document.body.appendChild(popup);
      }

      function closePopup(popup) {
        popup.style.opacity = '0';
        setTimeout(() => {
          popup.remove();
        }, 300);
      }
    </script>
    <script>
      document
        .getElementById('submitQuery')
        .addEventListener('click', async function () {
          // Gather form data
          const email = document.getElementById('emailInput').value;
          const name = document.getElementById('nameInput').value;
          const designation = document.getElementById('designationInput').value;
          const contact = document.getElementById('contactInput').value;
          const website = document.getElementById('websiteInput').value;
          const companyName = document.getElementById('companyNameInput').value;

          // Create the query object
          const queryData = {
            name: name,
            Designation: designation,
            companyName: companyName,
            Contact: contact,
            email: email,
            website: website,
          };

          try {
            // Send POST request to API
            const response = await fetch(
              'https://backend.gaganahuja.com/api/v1/queries',
              {
                method: 'POST',
                headers: {
                  'Content-Type': 'application/json',
                },
                body: JSON.stringify(queryData),
              }
            );

            // Parse response
            const result = await response.json();
            console.log(result);

            // Check if the query was created successfully
            if (response.ok) {
              alert('Query created successfully!');

              // Clear form fields after successful submission
              document.getElementById('emailInput').value = '';
              document.getElementById('nameInput').value = '';
              document.getElementById('designationInput').value = '';
              document.getElementById('contactInput').value = '';
              document.getElementById('websiteInput').value = '';
              document.getElementById('companyNameInput').value = '';
            } else {
              alert('Error creating query. Please try again.');
            }
          } catch (error) {
            console.error('Error:', error);
            alert('Error creating query. Please try again.');
          }
        });
    </script>
    <script>
      // Fetch the nav data from the API
      fetch('https://backend.gaganahuja.com/api/v1/navigation')
        .then((response) => response.json())
        .then((data) => {
          const navMenu = document.getElementById('nav-menu');

          // Iterate through the array of nav items
          data.forEach((item) => {
            if (item.isActive) {
              // Render only active items
              const listItem = document.createElement('li');
              const anchor = document.createElement('a');
              anchor.href = item.path; // Set the path
              anchor.textContent = item.label; // Set the label as text

              listItem.appendChild(anchor);
              navMenu.appendChild(listItem);
            }
          });
        })
        .catch((error) => {
          console.error('Error fetching navigation data:', error);
        });
    </script>

