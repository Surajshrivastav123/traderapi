              .map(
                (offering) => `
  <div class="item"> <i class="icon_check_alt2"></i>
    ${offering.description}
  </div>
`
              )
              .join('');
            const priceCard = `
                  <div class="col-xl-4 col-md-4">
                      <div class="price-card">
                          <div class="price-plan">
                              <div class="contents shape1 circle1">
                                  <div class="plan-type">${plan.titleTop}</div>
                                  <div class="price shapeTwo shapethree">${
                                    plan.title
                                  }</div>
                              </div>
                          </div>
                          <div class="price-details">
                              ${offerings}
                              <div class="item">
                                 <i class="icon_check_alt2"></i>  <strong class="highlight-price">Current Price: ${
                                   plan.price
                                 }/-</strong>
                              </div>
                                <button class="price-btn">
                               ${
                                 plan.paymentLink
                                   ? `<a href="${plan.paymentLink}" >Buy Now</a>`
                                   : `<a href='https://traderconvention.gaganahuja.com/register/${plan._id}'>BOOK NOW</a>`
                               }
                              </button>
                          </div>
                      </div>
                  </div>
              `;
            // Append each price card to the container
            pricingContainer.insertAdjacentHTML('beforeend', priceCard);
          });
        })
        .catch((error) => {
          console.error('Error fetching pricing data:', error);
        });
    </script>
    <script>
      document.addEventListener('DOMContentLoaded', function () {
        fetch('https://backend.gaganahuja.com/api/v1/home/')
          .then((response) => response.json())
          .then((data) => {
            if (data && data.length > 0) {
              const homeContent = data[0].home;
              // Parse the string to create a DOM element
              const parser = new DOMParser();

              // Find the about content section
              const aboutContentSection =
                document.querySelector('.about__content');
              if (aboutContentSection) {
                // Replace the content
                aboutContentSection.innerHTML = homeContent;
              }
            }
          })
          .catch((error) =>
            console.error('Error fetching home content:', error)
          );
      });
    </script>
    <script>
      // Fetch the nav data from the API
      fetch('https://backend.gaganahuja.com/api/v1/navigation')
        .then((response) => response.json())
        .then((data) => {
          const navMenu = document.getElementById('nav-menu');

          // Iterate through the array of nav items
          data.forEach((item) => {
            if (item.isActive) {
              // Render only active items
              const listItem = document.createElement('li');
              const anchor = document.createElement('a');
              anchor.href = item.path; // Set the path
              anchor.textContent = item.label; // Set the label as text

              listItem.appendChild(anchor);
              navMenu.appendChild(listItem);
            }
          });
        })
        .catch((error) => {
          console.error('Error fetching navigation data:', error);
        });
    </script>
  </body>
</html>
