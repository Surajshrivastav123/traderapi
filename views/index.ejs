    <script>
      document.addEventListener('DOMContentLoaded', function () {
        fetch('https://backend.gaganahuja.com/api/v1/speakers')
          .then((response) => response.json())
          .then((responseData) => {
            if (
              responseData.status === 200 &&
              Array.isArray(responseData.data)
            ) {
              const speakersContainer =
                document.getElementById('speakers-container');
              responseData.data.forEach((speaker) => {
                if (speaker.isActive && speaker.type === 'speaker') {
                  const speakerElement = createSpeakerElement(speaker);
                  speakersContainer.appendChild(speakerElement);
                }
              });
            } else {
              console.error('Unexpected API response structure:', responseData);
            }
          })
          .catch((error) => console.error('Error fetching speakers:', error));
      });

      function createSpeakerElement(speaker) {
        const speakerDiv = document.createElement('div');
        speakerDiv.className = 'speeker_fixed';

        // Transform the image path
        const imagePath = transformImagePath(speaker.img);

        speakerDiv.innerHTML = `
  <div class="instructor__content">
    <div class="instructor__image">
      <img src="${imagePath}" alt="${speaker.name}" loading="lazy">
      <div class="content__hover">
        <ul>
          <li>
            <h5 class="white-color">${speaker.name}</h5>
          </li>
        </ul>
<p class="iconsty">
  <a href="/profile/${speaker._id}" class="icon-link">
    <i class="fa fa-arrow-right"></i>
  </a>
</p>

      </div>
    </div>
    <div class="bottom-content">
      <h4><a href="#">${speaker.name}</a></h4>
    </div>
  </div>
`;

        return speakerDiv;
      }

      function transformImagePath(originalPath) {
        // Extract the filename from the original path
        const filename = originalPath.split('/').pop();

        // Construct the new URL
        return `https://backend.gaganahuja.com/uploads/${filename}`;
      }
    </script>
    <script>
      document
        .getElementById('submitQuery')
        .addEventListener('click', async function () {
          // Gather form data
          const email = document.getElementById('emailInput').value;
          const name = document.getElementById('nameInput').value;
          const designation = document.getElementById('designationInput').value;
          const contact = document.getElementById('contactInput').value;
          const website = document.getElementById('websiteInput').value;
          const companyName = document.getElementById('companyNameInput').value;

          // Create the query object
          const queryData = {
            name: name,
            Designation: designation,
            companyName: companyName,
            Contact: contact,
            email: email,
            website: website,
          };

          try {
            // Send POST request to API
            const response = await fetch(
              'https://backend.gaganahuja.com/api/v1/queries',
              {
                method: 'POST',
                headers: {
                  'Content-Type': 'application/json',
                },
                body: JSON.stringify(queryData),
              }
            );

            // Parse response
            const result = await response.json();
            console.log(result);

            // Check if the query was created successfully
            if (response.ok) {
              alert('Query created successfully!');

              // Clear form fields after successful submission
              document.getElementById('emailInput').value = '';
              document.getElementById('nameInput').value = '';
              document.getElementById('designationInput').value = '';
              document.getElementById('contactInput').value = '';
              document.getElementById('websiteInput').value = '';
              document.getElementById('companyNameInput').value = '';
            } else {
              alert('Error creating query. Please try again.');
            }
          } catch (error) {
            console.error('Error:', error);
            alert('Error creating query. Please try again.');
          }
        });
    </script>
    <script>
      // Fetch the pricing data from the backend API
      fetch('https://backend.gaganahuja.com/api/v1/eventPricing')
        .then((response) => response.json())
        .then((data) => {
          const pricingContainer = document.getElementById('pricing-cards');

          // Loop over each pricing plan and create card HTML
          data.forEach((plan) => {
