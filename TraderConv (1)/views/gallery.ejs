  <script>
    document.getElementById('submitQuery').addEventListener('click', async function () {
      // Gather form data
      const email = document.getElementById('emailInput').value;
      const name = document.getElementById('nameInput').value;
      const designation = document.getElementById('designationInput').value;
      const contact = document.getElementById('contactInput').value;
      const website = document.getElementById('websiteInput').value;
      const companyName = document.getElementById('companyNameInput').value;

      // Create the query object
      const queryData = {
        name: name,
        Designation: designation,
        companyName: companyName,
        Contact: contact,
        email: email,
        website: website
      };

      try {
        // Send POST request to API
        const response = await fetch('https://backend.gaganahuja.com/api/v1/queries', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify(queryData)
        });

        // Parse response
        const result = await response.json();
        console.log(result);

        // Check if the query was created successfully
        if (response.ok) {
          alert('Query created successfully!');

          // Clear form fields after successful submission
          document.getElementById('emailInput').value = '';
          document.getElementById('nameInput').value = '';
          document.getElementById('designationInput').value = '';
          document.getElementById('contactInput').value = '';
          document.getElementById('websiteInput').value = '';
          document.getElementById('companyNameInput').value = '';
        } else {
          alert('Error creating query. Please try again.');
        }
      } catch (error) {
        console.error('Error:', error);
        alert('Error creating query. Please try again.');
      }
    });
  </script>
  <script>
    document.addEventListener('DOMContentLoaded', () => {
      // Function to fetch gallery data
      async function fetchGallery() {
        try {
          const response = await fetch('https://backend.gaganahuja.com/api/v1/gallery/');
          if (!response.ok) throw new Error('Network response was not ok');
          const galleryItems = await response.json();
          renderGallery(galleryItems);
        } catch (error) {
          console.error('Error fetching gallery:', error);
        }
      }

      // Function to render gallery images
      function renderGallery(galleryItems) {
        const section1Container = document.querySelector('.section1 .row');
        const section2Container = document.querySelector('.section2 .row');
        const section3Container = document.querySelector('.section3 .row');

        galleryItems.forEach(item => {
          if (item.isActive) {
            const galleryElement = `
                <div class="col-lg-4 col-md-4 col-12">
                  <div class="gallery-image">
                    <img src="https://backend.gaganahuja.com/${item.image}" alt="Gallery Image">
                  </div>
                </div>
              `;

            // Append to the correct section based on category
            if (item.category === 'section 1') {
              section1Container.innerHTML += galleryElement;
            } else if (item.category === 'section 2') {
              section2Container.innerHTML += galleryElement;
            } else if (item.category === 'section 3') {
              section3Container.innerHTML += galleryElement;
            }
          }
        });
      }

      // Fetch and render gallery images on page load
      fetchGallery();
    });
  </script>

